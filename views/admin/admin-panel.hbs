<div class="page-wrapper">
    <div class="content">
        <div class="row">
            <div class="col-sm-5 col-5">
                <h4 class="page-title">Admin Panel</h4>
            </div>
            <div class="col-sm-7 col-7 text-right m-b-30">
                <a href="/admin/add-AdminUser" class="btn btn-primary btn-rounded"><i class="fa fa-plus"></i> Add Admin
                    User</a>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="table-responsive">
                    <table class="table table-striped custom-table mb-0 datatable">
                        <thead>
                            <tr>

                                <th class="text-center"> User Name</th>
                                <th> Created By</th>
                                <th>Action <b style="opacity: 0;">cewe</b> </th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each Admin_panel}}

                            <tr>

                                <td class="text-center">{{this.Username}}</td>
                                <td>{{this.CreatedBy}}</td>
                                <td>
                                    <div class="dropdown dropdown-action">
                                        <a href="#" class="action-icon dropdown-toggle" data-toggle="dropdown"
                                            aria-expanded="false"><i class="fa fa-ellipsis-v"></i> <b
                                                style="opacity: 0;">cewe</b> </a>
                                        <div class="dropdown-menu dropdown-menu-right">
                                            <a class="dropdown-item" href="/admin/delete-admin-user/{{this._id}}"
                                             onclick=" return confirm('Are You Sure to Delete {{this.Username}} User')"> Delete</a>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            {{/each}}

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>
</div>
</div>

</div>
<div class="sidebar-overlay" data-reff=""></div>